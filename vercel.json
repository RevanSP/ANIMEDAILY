{
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
    "redirects": [
      {
        "source": "/dashboard",
        "has": [
          {
            "type": "cookie",
            "key": "isLogin",
            "value": "false"
          }
        ],
        "destination": "https://animedaily.vercel.app/",
        "permanent": false
      }
    ]
  }
  